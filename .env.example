# Crawlab API Configuration
CRAWLAB_API_BASE_URL=http://localhost:8080/api
CRAWLAB_API_TOKEN=your_api_token_here

# MCP Server Configuration
MCP_SERVER_PORT=8000

# LLM Provider Configuration
# Choose one of: azure_openai, openai, anthropic, claude, together, groq, mistral, custom
LLM_PROVIDER_TYPE=azure_openai

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_API_VERSION=2023-05-15
AZURE_OPENAI_MODEL_NAME=your_deployment_name

# OpenAI Configuration
# OPENAI_API_KEY=your_api_key_here
# OPENAI_BASE_URL=https://api.openai.com/v1
# OPENAI_MODEL_NAME=gpt-3.5-turbo

# Anthropic/Claude Configuration
# ANTHROPIC_API_KEY=your_api_key_here
# ANTHROPIC_BASE_URL=https://api.anthropic.com
# ANTHROPIC_MODEL_NAME=claude-3-sonnet-20240229

# Together AI Configuration
# TOGETHER_API_KEY=your_api_key_here
# TOGETHER_BASE_URL=https://api.together.xyz
# TOGETHER_MODEL_NAME=togethercomputer/llama-2-70b-chat

# Groq Configuration
# GROQ_API_KEY=your_api_key_here
# GROQ_BASE_URL=https://api.groq.com/openai/v1
# GROQ_MODEL_NAME=llama-3-8b-8192

# Mistral Configuration
# MISTRAL_API_KEY=your_api_key_here
# MISTRAL_BASE_URL=https://api.mistral.ai/v1
# MISTRAL_MODEL_NAME=mistral-large-latest

# Custom OpenAI-compatible Provider
# CUSTOM_API_KEY=your_api_key_here
